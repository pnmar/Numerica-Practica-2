#include <iostream>
#include <fstream>
#include <cmath>
#include <vector>
using namespace std;

typedef tipo double
typedef vector v;

ifstream fileIn;
ofstream fileOut;

int main(int argc, char *arcv[]){
    
    //////////////////////////////////////
    // Lectura de datos de los ficheros //
    //////////////////////////////////////
    
    fileIn.open("MAT00.DAT");
    
    fileIn >> n >> m;
    v< v<tipo> > A(n,0);
    v<tipo> B(n,0);
    
    for (int i = 0; i < m; ++i){
        fileIn >> ind1 >> ind2 >> valor;
        A[ind1][ind2] = valor;
    }
    
    fileIn >> k;
    for (int i = 0; i < k; ++i){
        fileIn >> ind1 >> valor;
        B[ind1] = valor;
    }
    
    fileIn.close();
    
    
    
}
