#include <iostream>
#include <cmath>
#include "stdlib.h"
using namespace std;

/* PRE: A vector v of dimension n is given.
   POS: The norm_1 of the vector v is returned. */

double norm_inf(double *v, int n) {
    
    double sum = 0;
    for (int i = 0; i < n; ++i) {
        sum += abs(v[i]);
    }
    
    return sum;
    
}
